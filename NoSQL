# user_info 컬렉션 생성 | 21.12.30
db.createCollection("user_info",{capped:true, size:1000000})

# user_info 테스트 데이터 생성 | 21.12.30
db.user_info.insert({user_seq : 1, user_name : "오주현", user_email : "abcd@naver.com", user_password : 1234, user_address : "1동 1001호"})

# user_info 테스트 데이터 재생성 | 21.12.30
